function z=GD(u,fun,d)
    syms u1 u2 u3 u4 u5 u6                                 % 通过符号变量将匿名函数转换为符号函数
    str=[u1,u2,u3,u4,u5,u6];
    y = fun(str(1:d));
    for i=1:d
        z(i)
    end
    if d==1
        y = fun([u1])
        z = matlabFunction(diff(y,u1))
    end
    if d==2
        y = fun([u1,u2])
        z(1) = matlabFunction(diff(y,u1))
        
    end
    
    
end